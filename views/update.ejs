<%- include('./reuseComponent/logsign')%>
<body style="background-color: #007BFF;">
    <h1 style="color: white; text-align: center;">Connect With डिजिटल पाठशाला</h1>
    <div class="container">
        <h2>Enrollment Form</h2>
        <form action="/update/<%-dynamicData[0].id %>" method="POST">
            <label for="courseType">Course Type:</label>
            <input type="text" name="courseType"placeholder="Enter Course Type"value="<%-dynamicData[0].courseType %>"  required>

            <label for="courseName">Course Name:</label>
            <input type="text"  name="courseName"placeholder="Enter Course Name"value="<%-dynamicData[0].courseName %>" required>


            <input type="submit" value="Edit">
        </form>
    </div>
</body>
</html>
